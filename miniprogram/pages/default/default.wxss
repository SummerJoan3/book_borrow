.default{
  width: 80%;
  margin-left: 10%;
  border-bottom: 1rpx solid red;
  height: 150rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-family: "Verdana";  
}
.default .userInfo view{
  padding: 2rpx 0;
  font-size: 24rpx;
}
.default text{
  color: red;
  font-size: 30rpx;
}

.noSearch{
  width: 100%;
  height: 300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.noSearch cover-image{
  width: 100rpx;
  height: 100rpx;
}
.noSearch text{
  font-family: "Verdana";
  font-size: 32rpx;
}